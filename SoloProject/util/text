create table userinfo(
  userId varchar2(10 char) primary key,
  userPw varchar2(20 char) not null,
  userName varchar2(10 char) not null,
  userEmail varchar2(100 char),
  userAddress varchar2 (100 char) not null,
  userPhone number(15) not null
);


create table roominfo(
	roomNum number(10) primarykey,
	title varchar2(100 char) not null,
	userName varhacr2(10 char) not null
);


create sequence room_seq
increment by 1
minvalue 1
maxvalue 1000
nocycle
nocache
noorder;